<!--GSAP-->
<script>
window.addEventListener('load', function() {

    gsap.registerPlugin(ScrollTrigger);

    const initScrollTrigger = () => {
        const wrappers = document.querySelectorAll(".projects_thumb-wrapper");

        wrappers.forEach(wrapper => {
            const grad = wrapper.querySelector(".projects_thumb-grad");

            gsap.to(grad, {
                scrollTrigger: {
                    trigger: wrapper,
                    start: "top 60%",
                    end: "bottom 40%",
                    onEnter: () => gsap.to(grad, {
                        opacity: 1,
                        duration: 0.3,
                        ease: "Power1.easeInOut"
                    }),
                    onLeave: () => gsap.to(grad, {
                        opacity: 0,
                        duration: 0.3,
                        ease: "Power1.easeInOut"
                    }),
                    onEnterBack: () => gsap.to(grad, {
                        opacity: 1,
                        duration: 0.3,
                        ease: "Power1.easeInOut"
                    }),
                    onLeaveBack: () => gsap.to(grad, {
                        opacity: 0,
                        duration: 0.3,
                        ease: "Power1.easeInOut"
                    })
                }
            });
        });
    };

    // Initialize ScrollTrigger for initial elements
    initScrollTrigger();

    // CMS load event
    window.fsAttributes = window.fsAttributes || [];
    window.fsAttributes.push([
        'cmsload',
        (listInstances) => {
            console.log('cmsload Successfully loaded!');
            listInstances.forEach((instance) => {
                instance.on('renderitems', (renderedItems) => {
                    console.log('cmsload rendered!');
                    initScrollTrigger();
                    setTimeout(() => {
                        ScrollTrigger.refresh();
                    }, 100);
                });
            });
        },
    ]);

    // CMS filter event
    window.fsAttributes.push([
        'cmsfilter',
        (filterInstances) => {
            console.log('cmsfilter Successfully loaded!');
            filterInstances.forEach((instance) => {
                instance.listInstance.on('renderitems', (renderedItems) => {
                    console.log('cmsfilter rendered!');
                    initScrollTrigger();
                    setTimeout(() => {
                        ScrollTrigger.refresh();
                    }, 100);
                });
            });
        },
    ]);

});
</script></body></html>